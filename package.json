{
  "private": true,
  "description": "A datepicker library for React.",
  "repository": {
    "type": "git",
    "url": "git://github.com/maven-labs/react-datepicker.git"
  },
  "keywords": [
    "react",
    "datepicker",
    "styled-components",
    "styled-system",
    "hooks"
  ],
  "name": "@maven-labs/react-datepicker",
